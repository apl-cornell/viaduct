/* The classic MPC problem */
process main {
  a : {A};
  b : {B};
  b_richer : {(A | B)-> & (A & B)<-};

  a <- recv alice_main;
  b <- recv bob_main;

  a_endorsed : {A & B<-};
  b_endorsed : {B & A<-};

  a_endorsed = endorse(a, {A & B<-});
  b_endorsed = endorse(b, {B & A<-});

  b_richer = declassify(a_endorsed <= b_endorsed, {(A | B)-> & (A & B)<-});
  assert b_richer;
}

process alice_main {
  send 20 to main;
}

process bob_main {
  send 40 to main;
}

host alice : {A};
host bob : {B};

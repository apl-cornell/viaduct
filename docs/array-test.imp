process main {
    int{A} a[100];
    int{B} b[100];

    int{A} a_sum = 0;
    for (int ai = 0; ai < 100; ai++) {
        a_sum += a[ai];
    }

    int{B} b_sum = 0;
    for (int{B} bi = 0; bi < 100; bi++) {
        b_sum += b[bi];
    }

    int{A & B} scale = endorse(a_sum, {A & B<-}) + endorse(b_sum, {B & A<-});
    int{A & B} sum = 0;
    for (int{A & B} j = 0; j < 100; j++) {
        sum += scale * j;
    }

    int{A && B} prodsum = declassify(sum, {A && B});
}

host alice : {A};

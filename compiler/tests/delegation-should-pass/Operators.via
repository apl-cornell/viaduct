fun main() {
    /* Comparison Operators */
    assert 1 == 1;
    assert 1 != 2;

    assert 1 < 2;
    assert 1 <= 1;
    assert 1 <= 2;

    assert 2 > 1;
    assert 1 >= 1;
    assert 2 >= 1;

    /* Arithmetic Operators */
    assert -1 < 1;
    assert -1 + 1 == 0;

    assert 1 + 2 == 3;
    assert 3 - 1 == 2;
    assert 1 * 2 == 2;
    assert 2 / 1 == 2;

    assert min(1, 2) == 1;
    assert max(1, 2) == 2;

    /** Logical Operators */
    assert true;
    assert !false;
    assert true && true;
    assert false || true;
}


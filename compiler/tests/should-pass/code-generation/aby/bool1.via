host alice : {A}
host bob   : {A}

fun main() {
    let $aliceLocal@Local(host = alice) = input bool from alice;
    let $aliceLOr@Local(host = alice) = $aliceLocal;
    let $aliceLAnd@Local(host = alice) = $aliceLocal;
    let $aliceLNot@Local(host = alice) = $aliceLocal;

    let $bobLocal@Local(host = bob) = input bool from bob;
    let $bobLOr@Local(host = bob) = $bobLocal;
    let $bobLAnd@Local(host = bob) = $bobLocal;
    let $bobLNot@Local(host = bob) = $bobLocal;

    let $secretOr@BoolABY(client = bob, server = alice) = $aliceLOr || $bobLOr;
    let $secretAnd@BoolABY(client = bob, server = alice) = $aliceLAnd && $bobLAnd;
    let $secretNotAlice@BoolABY(client = bob, server = alice) = !$aliceLNot;
    let $secretNotBob@BoolABY(client = bob, server = alice) = !$bobLNot;

    let $clearOr@Replication(hosts = {alice, bob}) = $secretOr;
    let $clearAnd@Replication(hosts = {alice, bob}) = $secretAnd;
    let $clearNotAlice@Local(host = alice) = $secretNotAlice;
    let $clearNotBob@Local(host = bob) = $secretNotBob;

    output $clearOr to alice;
    output $clearOr to bob;
    output $clearAnd to alice;
    output $clearAnd to bob;
    output $clearNotAlice to alice;
    output $clearNotBob to bob;
}

process main {
    int{A} a1 <- recv @alice;
    int{A} a2 <- recv @alice;
    int{B} b1 <- recv @bob;
    int{B} b2 <- recv @bob;

    int{A} a;
    if (a1 < a2) {
        a = a1;
    } else {
        a = a2;
    }

    int{A} b;
    if (b1 < b2) {
        b = b1;
    } else {
        b = b2;
    }

}

host alice  : {A};
host bob    : {B};
